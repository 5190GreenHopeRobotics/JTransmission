#include "transmission.h"

int main() {
	packetBuffer p;
	p.addByte(0);
	p.addByte(2);
	p.addByte(1);
	p.addByte(12);
	transmissionPacket* tp;
	int size = p.getPackets(tp);
	if(size == 0) {
		delete[] tp;
		std::cout << "fail" << std::endl;
		return 1;
	}
	std::cout << "type:" << (short)tp[0].getMessageType() << std::endl;
	std::cout << "messageLength:" << (short)tp[0].getLength() << std::endl;
}


