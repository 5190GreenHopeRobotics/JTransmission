package net.viperfish.requestServer;

public class Request {
	private String src;
	private String type;
	private int srcConnection;
	private String description;
	
	public Request() {
		src = new String();
		description = new String();
		type = new String();
	}
	
	public void setSrcConn(int src) {
		srcConnection = src;
	}
	
	public void setSrc(String src) {
		this.src = src;
	}
	
	public void setDescritpion(String des) {
		this.description = des;
	}
	
	public String getSrc() {
		return src;
	}
	public String getDescritpion() {
		return description;
	}
	
	public int getSrcConn() {
		return srcConnection;
	}
}
