package org.usfirst.frc.team5190.sensorFilter;

public class ITG3200 {
	
	public static final int ITG_ADDR = 0xD0;
	public static final int WHO_AM_I = 0x00;
	public static final int SMPLRT_DIV = 0x15;
	public static final int DLPF_FS = 0x16;
	public static final int INT_CFG = 0x17;
	public static final int INT_STATUS = 0x1A;
	public static final int TEMP_OUT_H = 0X1B;
	public static final int TEMP_OUT_L = 0X1C;
	public static final int GYRO_XOUT_H = 0x1D;
	public static final int GYRO_XOUT_L = 0x1E;
	public static final int GYRO_YOUT_H = 0x1f;
	public static final int GYRO_YOUT_L = 0x20;
	public static final int GYRO_ZOUT_H = 0x21;
	public static final int GYRO_ZOUT_L = 0x22;
	public static final int PWR_MGM = 0x3E;
	public static final int DLPF_CFG_0 = 1<<0;
	public static final int DLPF_CFG_1 = 1 << 1;
	public static final int DLPF_CFG_2 = 1 << 2;
	public static final int DLPF_FS_SEL_0 = 1 << 3;
	public static final int DLPF_FS_SEL_1 = 1<<4 ;
	public static final int PWR_MGM_CLK_SEL_0 = 1 << 0;
	public static final int PWR_MGM_CLK_SEL_1 = 1<<1;
	public static final int PWR_MGM_CLK_SEL_2 = 1 << 2;
	public ITG3200(int port) {
		
	}
	
	public byte read(int addr) {
		
	}
	
	public void write(int addr, byte value) {
		
	}
	
	public double getDegree() {
		
	}
	
	protected void begin() {
		
	}
	
	protected void update() {
		
	}
}
