#include "transmission.h"
#include <iostream>
int main() {
	frc5190::vector<unsigned char> v;
	for(int i=0;i<20;++i) {
		unsigned char j = (short)i;
		v.append(j);
	}
	for(int i=0;i<v.getSize();++i) {
		std::cerr << "current vector:";
		for(int i=0;i<v.getSize();++i) {
			std::cerr << (short)v[i] << ",";
		}
		std::cerr << std::endl;
		std::cerr << "size:" << v.getSize() << std::endl;
		v.remove(0);
	}
}


